{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "format": "prettier --write \"**/*.{vue,ts,js,json,css,scss}\"",
    "format:check": "prettier --check \"**/*.{vue,ts,js,json,css,scss}\"",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint --ext .js,.ts,.vue .gitignore .",
    "lint:fix": "eslint --ext .js,.ts,.vue .gitignore . --fix"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.11.0",
    "@types/leaflet": "^1.9.18",
    "leaflet": "^1.9.4",
    "nuxt": "^3.17.4",
    "pinia": "^3.0.2",
    "vue": "^3.5.15",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^3.1.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/test-utils": "^2.4.6",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-vue": "^10.1.0",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0",
    "vitest": "^3.1.4",
    "vue-tsc": "^2.2.10"
  }
}
